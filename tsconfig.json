{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "sourceMap": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react-jsx",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "lib": [
            "es2021",
            "dom"
        ],
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["./src/**/*"]
  }